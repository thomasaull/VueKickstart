    "vue-router": "^3.0.6",
    "vue-wait": "^1.4.6",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@ascendancyy/vue-cli-plugin-stylelint": "^1.1.2",
    "@nuxtjs/style-resources": "^0.1.2",
