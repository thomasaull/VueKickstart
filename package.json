{
  "name": "vue-kickstart",
  "version": "0.0.0",
  "scripts": {
    "vue:serve": "vite",
    "vue:build": "run-p type-check build-only",
    "vue:preview": "vite preview --port 4173",
    "astro:serve": "astro dev",
    "astro:build": "astro build",
    "astro:preview": "astro preview",
    "storybook:serve": "storybook dev -p 6006 --no-open",
    "storybook:build": "storybook build",
    "test:unit": "vitest --environment jsdom",
    "test:e2e": "start-server-and-test preview http://localhost:4173/ 'cypress open --e2e'",
    "test:e2e:ci": "start-server-and-test preview http://localhost:4173/ 'cypress run --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:style": "stylelint **/*.{vue,htm,html,css,sss,less,scss}",
    "lint:style:fix": "npm run lint:style -- --fix",
    "format": "prettier --write src/",
    "generate:graphql": "graphql-codegen",
    "generate:xstate": "xstate typegen 'src/**/*.ts?(x)'",
    "generate:meta": "vite-node ./scripts/generateComponentMeta.ts -- generate",
    "generate:meta:watch": "npm run generate:meta -- --watch",
    "postinstall": "rm -rf ./node_modules/@types/react",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/client": "^3.6.9",
    "@astrojs/vue": "^2.1.0",
    "@vue/apollo-composable": "^4.0.0-alpha.19",
    "@xstate/inspect": "^0.7.0",
    "@xstate/vue": "^2.0.0",
    "astro": "^2.1.1",
    "cuid": "^2.1.8",
    "graphql": "^16.5.0",
    "graphql-tag": "^2.12.6",
    "gsap": "^3.10.4",
    "include-media": "^1.4.10",
    "lazysizes": "^5.3.2",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.32",
    "portal-vue": "^3.0.0-beta.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "what-input": "^5.2.12",
    "xstate": "^4.32.1"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@graphql-codegen/cli": "^2.11.3",
    "@graphql-codegen/typed-document-node": "^2.3.2",
    "@graphql-codegen/typescript": "^2.7.2",
    "@graphql-codegen/typescript-operations": "^2.5.2",
    "@rollup/plugin-graphql": "^1.1.0",
    "@rushstack/eslint-patch": "^1.2.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/gitlab": "^9.4.1",
    "@storybook/addon-actions": "^7.0.0-beta.61",
    "@storybook/addon-essentials": "^7.0.0-beta.61",
    "@storybook/addon-interactions": "^7.0.0-beta.61",
    "@storybook/addon-links": "^7.0.0-beta.61",
    "@storybook/addon-mdx-gfm": "^7.0.0-beta.61",
    "@storybook/blocks": "^7.0.0-beta.61",
    "@storybook/builder-vite": "^7.0.0-beta.61",
    "@storybook/testing-library": "^0.0.14-next.1",
    "@storybook/vue3": "^7.0.0-beta.61",
    "@storybook/vue3-vite": "^7.0.0-beta.61",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.14.2",
    "@types/prettier": "^2.7.2",
    "@vitejs/plugin-vue": "^4.0.0",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.2",
    "@vue/test-utils": "^2.3.0",
    "@vue/tsconfig": "^0.1.3",
    "@xstate/cli": "^0.4.2",
    "babel-loader": "^8.2.5",
    "chokidar": "^3.5.3",
    "commander": "^10.0.0",
    "cypress": "^10.3.0",
    "esbuild-register": "^3.3.3",
    "eslint": "^8.34.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-vue": "^9.9.0",
    "husky": "^8.0.1",
    "jsdom": "^21.1.0",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.54.0",
    "semantic-release": "^19.0.3",
    "semantic-release-gitmoji": "^1.4.4",
    "start-server-and-test": "^1.14.0",
    "storybook": "^7.0.0-beta.61",
    "stylelint": "^14.9.1",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-config-recommended-vue": "^1.4.0",
    "stylelint-config-sass-guidelines": "^9.0.1",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-config-standard-scss": "^5.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^4.3.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "typescript": "~4.8.4",
    "vite": "^4.1.4",
    "vite-svg-loader": "^3.4.0",
    "vitest": "^0.29.1",
    "vue-component-meta": "^1.2.0",
    "vue-loader": "^16.8.3",
    "vue-tsc": "^1.2.0"
  }
}
